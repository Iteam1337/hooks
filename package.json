{
  "name": "@opendevtools/hooks",
  "version": "0.5.5",
  "main": "dist/hooks.cjs.js",
  "module": "dist/hooks.esm.js",
  "types": "dist/types/index.d.ts",
  "description": "A collection of reusable hooks",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "npm run build && husky install",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:ci": "jest",
    "coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js"
  },
  "keywords": [],
  "author": "believer <rickard.laurin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "query-string": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/react-hooks": "5.1.2",
    "@types/jest": "26.0.23",
    "@types/query-string": "^6.3.0",
    "@types/react": "17.0.5",
    "eslint": "7.26.0",
    "husky": "^6.0.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^26.6.2",
    "jest-environment-jsdom-global": "^2.0.4",
    "jest-localstorage-mock": "2.4.12",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "2.50.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "bugs": {
    "url": "https://github.com/opendevtools/hooks/issues"
  },
  "homepage": "https://github.com/opendevtools/hooks#readme",
  "repository": {
    "type": "git",
